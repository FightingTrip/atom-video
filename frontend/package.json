{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "dev": "vite --mode development",
    "dev:mock": "vite --mode mock",
    "dev:api": "cross-env VITE_USE_MOCK=false vite --mode development",
    "clean:src:unix": "find src -type f -name \"*.js\" -o -name \"*.js.map\" -o -name \"*.vue.d.ts\" -o -name \"*.d.ts\" ! -name \"env.d.ts\" ! -path \"*/types/*\" -delete",
    "clean:src:win": "powershell -File ./clean-src.ps1",
    "clean:src": "node -e \"process.platform === 'win32' ? process.exit(1) : process.exit(0)\" && npm run clean:src:unix || npm run clean:src:win",
    "clean:dist": "rimraf dist",
    "clean": "npm run clean:dist && npm run clean:src",
    "build": "npm run clean && vite build",
    "build:test": "npm run clean && vite build --mode test",
    "build:prod": "npm run clean && vite build --mode production",
    "build:check": "npm run clean && vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\" --fix",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@atom/shared-types": "workspace:*",
    "@faker-js/faker": "^9.6.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@vicons/ionicons5": "^0.13.0",
    "@vueuse/core": "^13.0.0",
    "axios": "^1.8.4",
    "chart.js": "^4.4.8",
    "dayjs": "^1.11.13",
    "naive-ui": "^2.41.0",
    "pinia": "^3.0.1",
    "postcss-preset-env": "10.1.5",
    "sass": "1.86.3",
    "vue": "^3.4.15",
    "vue-i18n": "12.0.0-alpha.2",
    "vue-plyr": "7.0.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@atom/eslint-config": "workspace:*",
    "@atom/tsconfig": "workspace:*",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@intlify/unplugin-vue-i18n": "6.0.5",
    "@storybook/addon-essentials": "^7.6.12",
    "@storybook/addon-interactions": "^7.6.12",
    "@storybook/addon-links": "^7.6.12",
    "@storybook/blocks": "^7.6.12",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^7.6.12",
    "@storybook/vue3-vite": "^7.6.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "postcss": "^8.4.35",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.3",
    "resolve-url-loader": "5.0.0",
    "rimraf": "^5.0.5",
    "storybook": "^7.6.12",
    "stylelint": "^16.17.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^37.0.0",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^3.4.1",
    "terser": "^5.39.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.5",
    "vite-plugin-vue-type-imports": "0.2.5",
    "vitest": "3.1.1",
    "vue-tsc": "^2.2.4"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "> 0.2%",
    "maintained node versions",
    "current node",
    "not ie 11",
    "not op_mini all"
  ]
}
