{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "dev": "vite --mode development",
    "dev:mock": "vite --mode mock",
    "dev:api": "cross-env VITE_USE_MOCK=false vite --mode development",
    "build": "vue-tsc -b ; vite build",
    "build:test": "vue-tsc -b ; vite build --mode test",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\" --fix",
    "format": "prettier --write \"src/**/*.{js,ts,vue,css,scss}\"",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@vueuse/core": "^13.0.0",
    "axios": "^1.8.4",
    "chart.js": "^4.4.8",
    "naive-ui": "^2.41.0",
    "pinia": "^3.0.1",
    "vue": "^3.4.15",
    "vue-i18n": "12.0.0-alpha.2",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-legacy": "^6.0.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "postcss": "^8.4.35",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.3",
    "stylelint": "^16.17.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^37.0.0",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^3.4.1",
    "terser": "^5.39.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vue-tsc": "^2.2.4"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "> 0.2%",
    "maintained node versions",
    "current node",
    "not ie 11",
    "not op_mini all"
  ]
}
