# 生产环境配置

# API 配置
VITE_API_URL=https://api.atomvideo.com/api
VITE_WS_URL=wss://api.atomvideo.com/ws

# 是否使用模拟数据
VITE_USE_MOCK=false

# 应用配置
VITE_APP_TITLE=Atom Video
VITE_APP_ENV=production

# 调试配置
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_LOGGING=false

# OAuth 配置
VITE_GOOGLE_CLIENT_ID=your-google-client-id-prod
VITE_GITHUB_CLIENT_ID=your-github-client-id-prod

# 性能监控
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_SENTRY_DSN=your-sentry-dsn-prod

# 缓存配置
VITE_ENABLE_CACHE=true
VITE_CACHE_DURATION=7200 