# 测试环境配置

# API 配置
VITE_API_URL=http://test-api.atomvideo.com/api
VITE_WS_URL=ws://test-api.atomvideo.com/ws

# 是否使用模拟数据
VITE_USE_MOCK=true

# 应用配置
VITE_APP_TITLE=Atom Video (Test)
VITE_APP_ENV=test

# 调试配置
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_LOGGING=true

# OAuth 配置
VITE_GOOGLE_CLIENT_ID=your-google-client-id-test
VITE_GITHUB_CLIENT_ID=your-github-client-id-test

# 性能监控
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_SENTRY_DSN=your-sentry-dsn-test

# 缓存配置
VITE_ENABLE_CACHE=true
VITE_CACHE_DURATION=1800 